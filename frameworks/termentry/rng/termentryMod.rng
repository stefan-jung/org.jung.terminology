    <define name="genderSelection-atts">
      <optional>
        <attribute name="gender">
          <choice>
            <value>neuter</value>
            <value>masculine</value>
            <value>feminine</value>
          </choice>
        </attribute>
      </optional>
    </define>
    <define name="caseSelection-atts">
      <attribute name="case" a:defaultValue="nominative">
        <choice>
          <value>nominative</value>
          <value>accusative</value>
          <value>dative</value>
          <value>genitive</value>
        </choice>
      </attribute>
    </define>
    <define name="numberSelection-atts">
      <attribute name="number" a:defaultValue="singular">
        <choice>
          <value>singular</value>
          <value>plural</value>
        </choice>
      </attribute>
    </define>
    <define name="partOfSpeech-atts">
      <attribute name="value" a:defaultValue="noun">
        <a:documentation>The part of speech of the term.</a:documentation>
        <choice>
          <value>noun</value><a:documentation>Noun</a:documentation>
          <value>pronoun</value><a:documentation>pronoun</a:documentation>
          <value>adjective</value><a:documentation>Adjective</a:documentation>
          <value>verb</value><a:documentation>Verb</a:documentation>
          <value>adverb</value><a:documentation>Adverb</a:documentation>
          <value>preposition</value><a:documentation>Preposition</a:documentation>
