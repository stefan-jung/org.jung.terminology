    <moduleMetadata>
      <moduleType>topic</moduleType>
      <moduleShortName>semanticnet</moduleShortName>
      <modulePublicIds>
        <dtdMod>-//DOCTALES//ELEMENTS DITA<var presep=" " name="ditaver"/> Semantic Net//EN</dtdMod>
        <dtdEnt>-//DOCTALES//ENTITIES DITA<var presep=" " name="ditaver"/> Semantic Net//EN</dtdEnt>
        <xsdMod>urn:doctales:dita:xsd:semanticnetMod.xsd<var presep=":" name="ditaver"/></xsdMod>
        <xsdGrp>urn:doctales:dita:xsd:semanticnetGrp.xsd<var presep=":" name="ditaver"/></xsdGrp>
        <rncMod>urn:doctales:dita:rnc:semanticnetMod.rnc<var presep=":" name="ditaver"/></rncMod>
        <rngMod>urn:doctales:dita:rng:semanticnetMod.rng<var presep=":" name="ditaver"/></rngMod>
