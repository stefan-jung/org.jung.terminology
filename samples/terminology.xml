<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="https://www.dita-ot.org/project">
  
  <context name="Terminology" id="terminology">
    <input href="terminology.ditamap"/>
  </context>
  
  <deliverable name="Termbrowser" id="termbrowser">
    <context idref="terminology"/>
    <output href="./termbrowser"/>
    <publication transtype="termbrowser-responsive">
      <param name="webhelp.publishing.template" path="../publishing"/>
      <param name="webhelp.publishing.template.descriptor" value="termbrowser.opt"/>
      <param name="args.termstats" href="termstats.xml"/>
      <param name="debugging.mode" value="true"/>
      <param name="clean.temp" value="no"/>
    </publication>
  </deliverable>
  
  <deliverable name="Termchecker DITA" id="termchecker-dita">
    <context idref="terminology"/>
    <output href="./termchecker-dita"/>
    <publication transtype="termchecker-dita">
      <!--<param name="debugging.mode" value="true"/>
      <param name="clean.temp" value="no"/>
      <param name="dita.temp.dir" href="temp/termchecker-dita/"/>-->
    </publication>
  </deliverable>
  
  <deliverable name="Termchecker XLIFF" id="termchecker-xliff">
    <context idref="terminology"/>
    <output href="./termchecker-xliff"/>
    <publication transtype="termchecker-xliff"/>
  </deliverable>
  
  <deliverable name="TBX Basic v2" id="tbx-basic-v2">
    <context idref="terminology"/>
    <output href="./tbx-basic-v2"/>
    <publication transtype="tbx-basic-v2">
      <param name="debugging.mode" value="true"/>
      <param name="clean.temp" value="no"/>
    </publication>
  </deliverable>
  
  <deliverable name="TBX Basic v3" id="tbx-basic-v3">
    <context idref="terminology"/>
    <output href="./tbx-basic-v3"/>
    <publication transtype="tbx-basic-v3">
      <param name="debugging.mode" value="true"/>
      <param name="clean.temp" value="no"/>
      <param name="main.language" value="en-GB"/>
    </publication>
  </deliverable>
  
  <deliverable name="TBX Min v3 DCA" id="tbx-min-v3-dca">
    <context idref="terminology"/>
    <output href="./tbx-min-v3-dca"/>
    <publication transtype="tbx-min-v3-dca">
      <param name="source.language" value="en-US"/>
      <param name="license" value="APL2"/>
    </publication>
  </deliverable>
  
  <deliverable name="TBX Min v3 DCT" id="tbx-min-v3-dct">
    <context idref="terminology"/>
    <output href="./tbx-min-v3-dct"/>
    <publication transtype="tbx-min-v3-dct">
      <param name="source.language" value="en-US"/>
      <param name="license" value="APL2"/>
    </publication>
  </deliverable>
  
  <deliverable name="Multiterm XML" id="multiterm">
    <context idref="terminology"/>
    <output href="./multiterm"/>
    <publication transtype="multiterm">
      <param name="debugging.mode" value="true"/>
    </publication>
  </deliverable>
  
  <deliverable name="Oxygen Termchecker" id="oxygen-termchecker">
    <context idref="terminology"/>
    <output href="./oxygen-termchecker"/>
    <publication transtype="oxygen-termchecker"/>
  </deliverable>
  
</project>
